#include "stdafx.h"
#include <iostream>
#include <string>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	string str,str1;
	cout << "Введите строку: ";
	cin >> str;
	cout << "Введите строку: ";
	cin >> str1;
	int n = str.size(), n1 = str1.size(),x=0;
	for (int i = 0;(i < n)||(i < n1);i++) {
		if (str[i] == str1[i]) {
			x++;
		}
	}
	cout << x << endl;
}
