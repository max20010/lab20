#include <iostream>
#include <string>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	string str,str1,c="1";
	//char c;
	cout << "Введите строку: ";
	cin >> str;
	cout << "Введите символ: ";
	//cin >> c;
	int n = str.size();
	for (int i=0,j=0;i<7;j++) {
		if (str[j] == '1') {
			str1[i] = '1';
			i++;
			str1[i] = '1';
		}else
		{
			str1[i] = str[j];
		}
		i++;
	}
	cout << str1 << endl;
}
